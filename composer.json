{
  "name": "lenafo/sage-woocommerce-starter",
  "description": "Sage 10 + Tailwind CSS + WooCommerce starter theme",
  "type": "wordpress-theme",
  "require": {
    "php": ">=8.0",
    "roots/sage": "^10.0",
    "generoi/sage-woocommerce": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "SageWooCommerce\\": "vendor/generoi/sage-woocommerce/src"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "wp acorn package:discover"
    ]
  }
}
